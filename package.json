    "@types/node-fetch": "2.5.10",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "next-pwa": "^5.4.4",
    "next-sitemap": "^2.0.7",
    "postcss": "^8.4.5",
