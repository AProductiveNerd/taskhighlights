{
  "name": "taskhighlights2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "yarn next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "prett": "prettier -w .",
    "es": "eslint src --ignore-path .eslintignore",
    "final": "yarn prett && yarn es && yarn lint && yarn build && yarn postbuild",
    "prod": "yarn prett && yarn es && yarn lint && yarn build && yarn postbuild && yarn start",
    "devMigrate": "yarn prisma migrate dev",
    "prodMigrate": "yarn prisma migrate deploy",
    "introspect": "yarn prisma db pull",
    "generate": "yarn prisma generate",
    "flush": "yarn devMigrate && yarn introspect && yarn generate"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.8.1",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dexie": "^3.2.0",
    "firebase": "^9.6.4",
    "fuse.js": "^6.5.3",
    "next": "^12.0.8",
    "next-plugin-preact": "^3.0.6",
    "next-seo": "^4.29.0",
    "node-fetch": "2.6.1",
    "preact": "^10.6.4",
    "preact-render-to-string": "^5.1.19",
    "prisma": "^3.8.1",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-hotkeys-hook": "^3.4.3",
    "react-loading-skeleton": "^3.0.2",
    "react-nice-avatar": "1.2.2",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
